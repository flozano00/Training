function list_portlet(portlet,column){
	try{
	portlet.setTitle("Form Porlet");
	portlet.addField("custpage_name", "text", "Name"); //Name
	portlet.addField("custpage_age", "decimal", "age"); //Age
	portlet.addField("custpage_lastName", "text", "Last Name"); //Last Name

	}
	catch(e){
	nlapiLogExecution("Debug", "Catch Exception", "Exception" + e);
	}

}